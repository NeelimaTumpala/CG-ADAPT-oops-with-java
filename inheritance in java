package co.vinod;
import co.vinod.entity.Television;

public class Program01{
  
  pulbic static void main(String[] args){
    
    Television tv1= new Television();
    tv1.setId()
    tv1.setName("vivo");
    tv1.setPrice(50000.0);
    tv1.setScrentype("led");
    tv1.setScreensize(20)
    
    tv1.print()
  }
}
